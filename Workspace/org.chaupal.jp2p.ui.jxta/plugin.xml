<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="net.jxta.platform.Module"
            class="org.chaupal.jp2p.ui.jxta.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="net.jxta.platform.NetworkManager"
            class="org.chaupal.jp2p.ui.jxta.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="net.jxta.document.Advertisement"
            class="org.chaupal.jp2p.ui.jxta.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="net.jxta.platform.NetworkConfigurator"
            class="org.chaupal.jp2p.ui.jxta.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.jxse.ui.connectivity"
            name="JP2P Connectivity Monitor"
            parentCategory="org.eclipse.ui">
      </category>
      <view
            allowMultiple="false"
            category="org.chaupal.jp2p.ui.container"
            class="org.chaupal.jp2p.ui.jxta.peergroup.PeerGroupNavigator"
            icon="resources/peergroup.png"
            id="org.chaupal.jp2p.ui.context.peergroups"
            name="JP2P PeerGroup Viewer"
            restorable="true">
      </view>
      <view
            category="org.eclipse.jxse.ui.connectivity"
            class="org.chaupal.jp2p.ui.jxta.monitor.ConnectivityViewPart"
            icon="resources/connection.png"
            id="org.eclipse.jxse.ui.monitor.ConnectivityViewPart"
            name="JP2P Connectivity Monitor">
      </view>
      <view
            allowMultiple="true"
            category="org.eclipse.jxse.ui.connectivity"
            class="org.chaupal.jp2p.ui.jxta.view.ModuleTableViewer"
            icon="resources/component.png"
            id="org.eclipse.jxse.ui.view.ModuleTableViewer"
            name="JP2P Module Viewer ">
      </view>
      <view
            class="org.chaupal.jp2p.ui.jxta.advertisement.AdvertisementTableViewer"
            icon="resources/advertisement2.png"
            id="org.eclipse.jxse.ui.view.AdvertisementTableViewer"
            name="Advertisement Viewer"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.chaupal.jp2p.ui.jxta.view.advertisements.AdvertisementNavigator"
            icon="resources/advertisement.png"
            id="org.condast.jxta.ide.view.jxta.advertisements"
            name="JP2P Advertisement Viewer"
            restorable="true">
      </view>
      <view
            class="org.chaupal.jp2p.ui.jxta.rendezvous.RendezvousServiceViewPart"
            icon="resources/rendezvous.png"
            id="org.chaupal.jp2p.ui.rendezvous.RendezvousServiceViewPart"
            name="Rendezvous Service Viewer">
      </view>
      <view
            class="org.chaupal.jp2p.ui.jxta.endpoint.EndPointViewPart"
            icon="resources/endpoint.png"
            id="org.chaupal.jp2p.ui.endpoint.view"
            name="Endpoint Viewer"
            restorable="true">
      </view>
      <view
            category="org.chaupal.jp2p.ui.container"
            class="org.chaupal.jp2p.ui.jxta.peergroup.PeerGroupViewPart"
            icon="resources/peergroup.png"
            id="org.chaupal.jp2p.ui.peergroup.view"
            name="PeerGroup Viewer"
            restorable="true">
      </view>
   </extension>
   <extension
         id="org.eclipse.jxse.content"
         name="JXSE Content"
         point="org.eclipse.ui.navigator.navigatorContent">
       <navigatorContent
            activeByDefault="true"
            contentProvider="org.chaupal.jp2p.ui.jxta.provider.NetworkConfiguratorContentProvider"
            id="org.eclipse.jxse.content.network.configurator"
            labelProvider="org.chaupal.jp2p.ui.jxta.provider.Jp2pLabelProvider"
            name="JP2P Network Configurator Content"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="net.jxta.platform.NetworkConfigurator">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="java.net.URI">
               </instanceof>
               <instanceof
                     value="java.security.cert.X509Certificate">
               </instanceof>
            </or>
         </possibleChildren>
      </navigatorContent>
      <navigatorContent
            contentProvider="org.chaupal.jp2p.ui.jxta.view.advertisements.Jp2pAdvertisementContentProvider"
            id="org.chaupal.jp2p.ui.view.advertisements"
            labelProvider="org.chaupal.jp2p.ui.jxta.view.advertisements.JxseAdvertisementLabelProvider"
            name="JP2P Advertisement Content"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="net.jp2p.container.component.IJp2pComponent">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="net.jxta.document.Advertisement">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.chaupal.jp2p.ui.jxta.peergroup.PeerGroupContentProvider"
            id="org.chaupal.jp2p.content.peergroup"
            labelProvider="org.chaupal.jp2p.ui.jxta.peergroup.PeerGroupLabelProvider"
            name="JP2P PeerGroup Container"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="net.jp2p.container.utils.SimpleNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="net.jp2p.container.utils.SimpleNode">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
  </extension>
   <extension
         id="org.condast.jxta.ide.view.jxta.container.viewer"
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="org.eclipselabs.jxse.ui.context.peergroups">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="org.eclipse.jxse.content.peergroup">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
    <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.chaupal.jp2p.ui.jxta.perspective.Perspective"
            fixed="false"
            id="org.eclipse.jxse.perspective"
            name="JP2P IDE Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jxse.perspective">
         <view
               closeable="true"
               id="org.chaupal.jp2p.ui.context.view"
               minimized="false"
               moveable="true"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               relationship="bottom"
               relative="org.chaupal.jp2p.ui.context.view"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.chaupal.jp2p.ui.peergroup.view"
               minimized="false"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.console.ConsoleView"
               minimized="false"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               id="org.eclipse.ui.views.ProblemView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.console.ConsoleView">
         </view>
         <view
               id="org.eclipse.jxse.ui.monitor.ConnectivityViewPart"
               minimized="false"
               relationship="bottom"
               relative="org.chaupal.jp2p.ui.peergroup.view"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.jxse.ui.view.ModuleTableViewer"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.jxse.ui.monitor.ConnectivityViewPart"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.jxse.ui.view.AdvertisementTableViewer"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.jxse.ui.view.ModuleTableViewer"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               id="org.chaupal.jp2p.ui.rendezvous.RendezvousServiceViewPart"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.jxse.ui.view.AdvertisementTableViewer">
         </view>
         <view
               closeable="true"
               id="org.chaupal.jp2p.ui.endpoint.view"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.chaupal.jp2p.ui.rendezvous.RendezvousServiceViewPart"
               showTitle="true"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
</plugin>
